<h1>Fotosession</h1>
<form action="/visit" method="POST" role="form">
  
  <div class="form-group">
    <label>Name</label>
    <input name="username" type="text" class="form-control"  placeholder="Enter your name">
  </div>
 <div class="form-group">
    <label>datetime</label>
    <input name="datetime" type="datetime" class="form-control" placeholder="enter datetime">
  </div>

 <div class="form-group">
    <label>Photografer</label>
    
 <select class="custom-select">
    <option name="photografer" selected>Choose Photografer</option>
    <option value="1">Gleb Smirnoff</option>
    <option value="2">Ditriy</option>
    <option value="3">Sasha</option>
  </select>
   </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>
